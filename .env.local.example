# Optimisations pour les builds Next.js 15 avec Stack Auth
NODE_OPTIONS=--max-old-space-size=4096
SKIP_ENV_VALIDATION=true
NEXT_TELEMETRY_DISABLED=1

# Variables d'environnement requises
GEMINI_API_KEY=your_gemini_api_key_here
QLOO_API_KEY=your_qloo_api_key_here
QLOO_API_URL=https://api.qloo.com

# Stack Auth
STACK_PROJECT_ID=your_stack_project_id
STACK_PUBLISHABLE_CLIENT_KEY=your_stack_client_key
STACK_SECRET_SERVER_KEY=your_stack_server_key

# Base de donn√©es
DATABASE_URL=your_database_url_here

# Qloo-First Persona Generation Feature Flags
QLOO_FIRST_GENERATION_ENABLED=false
QLOO_FIRST_FALLBACK_ENABLED=true
QLOO_FIRST_DEBUG_MODE=false

# Qloo-First Performance Tuning
QLOO_FIRST_BATCH_SIZE=5
QLOO_FIRST_PARALLEL_REQUESTS=3
QLOO_FIRST_CACHE_TTL=3600